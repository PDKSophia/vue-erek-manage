{
  "name": "vue-manage",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "wiki-docs:dev": "vuepress dev wiki-docs",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "eslint-fix": "eslint src/**/**/*.vue --fix",
    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\""
  },
  "dependencies": {
    "apollo-tracing": "^0.5.2",
    "axios": "^0.18.0",
    "echarts": "^4.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "iview": "^3.1.3",
    "js-model": "^1.6.1",
    "ts-node": "^8.0.3",
    "core-js": "^2.6.5",
    "vue": "^2.6.6",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.9.3",
    "prettier": "^1.15.3",
    "prettier-eslint-cli": "^4.7.1",
    "sass-loader": "^7.1.0",
    "vuepress": "^1.9.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-dupe-args": 2,
      "no-irregular-whitespace": 2,
      "generator-star-spacing": "off",
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "none",
          "bracketSpacing": true,
          "jsxBracketSameLine": true,
          "htmlWhitespaceSensitivity": "strict"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}